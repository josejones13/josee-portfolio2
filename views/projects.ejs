<%- include('partials/header') %>
<h2>Projects</h2>
<div class="row g-4">
  <% projects.forEach(function(p){ %>
    <div class="col-md-4">
      <div class="card h-100">
        <img src="<%= p.image %>" class="card-img-top" alt="<%= p.title %>">
        <div class="card-body">
          <h5 class="card-title"><%= p.title %></h5>
          <p class="card-text"><%= p.summary %></p>
        </div>
      </div>
    </div>
  <% }) %>
</div>
<%- include('partials/footer') %>